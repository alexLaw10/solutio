{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/solutio-ds/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "stencil build",
        "cwd": "libs/solutio-ds"
      },
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/loader"
      ]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "stencil test --spec",
        "cwd": "libs/solutio-ds"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/solutio-ds/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
